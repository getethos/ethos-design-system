  "license": "MIT",
  "dependencies": {
    "js-cookie": "latest",
    "node-sass": "latest"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "faker": "^4.1.0",
    "json-schema-faker": "^0.5.0-rc23",
    "json-server": "^0.15.1",
    "prettier": "^1.18.2",
